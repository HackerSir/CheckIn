{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "anlutro/l4-settings": "^0.5.0",
        "arcanedev/log-viewer": "^4.3",
        "backup-manager/laravel": "^1.2",
        "barryvdh/laravel-ide-helper": "^2.3",
        "danny50610/imgur-laravel": "dev-master",
        "doctrine/dbal": "^2.5",
        "graham-campbell/throttle": "^5.3",
        "guzzlehttp/guzzle": "^6.3",
        "iatstuti/laravel-nullable-fields": "^1.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "lavary/laravel-menu": "^1.6",
        "milon/barcode": "^5.3",
        "mojoblanco/domainos": "^1.1",
        "phpoffice/phpspreadsheet": "dev-develop",
        "phpoffice/phpword": "dev-master",
        "pragmarx/google2fa-laravel": "^0.1.2",
        "predis/predis": "^1.1",
        "recca0120/laravel-tracy": "^1.8",
        "roumen/sitemap": "^2.6",
        "santigarcor/laratrust": "^3.2",
        "thomaswelton/laravel-gravatar": "^1.1",
        "tom-lingham/searchy": "^2.0",
        "yajra/laravel-datatables-buttons": "^1.3",
        "yajra/laravel-datatables-html": "^1.2",
        "yajra/laravel-datatables-oracle": "^7.6",
        "yish/generators": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/danny50610/Imgur-Laravel"
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
