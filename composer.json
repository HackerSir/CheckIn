{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "ext-PDO": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "anlutro/l4-settings": "^1.0",
        "arcanedev/log-viewer": "^5.0",
        "backup-manager/laravel": "^1.3",
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.9",
        "dyrynda/laravel-nullable-fields": "^2.0",
        "fideloper/proxy": "^4.1",
        "graham-campbell/throttle": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.4",
        "laravel/horizon": "^3.1",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "laravelium/sitemap": "6.*",
        "lavary/laravel-menu": "^1.7",
        "maatwebsite/excel": "^3.1",
        "marvinlabs/laravel-html-bootstrap-4": "^1.6",
        "marvinlabs/laravel-html-font-awesome": "^1.0",
        "mews/purifier": "^3.2",
        "milon/barcode": "^6.0",
        "mojoblanco/domainos": "^1.1",
        "phpoffice/phpspreadsheet": "^1.6",
        "phpoffice/phpword": "^0.16.0",
        "pragmarx/google2fa-laravel": "^1.0",
        "predis/predis": "^1.1",
        "recca0120/laravel-tracy": "^1.8",
        "santigarcor/laratrust": "^5.2",
        "spatie/laravel-activitylog": "^3.7",
        "spatie/laravel-google-calendar": "^2.2.0",
        "thomaswelton/laravel-gravatar": "^1.1",
        "tom-lingham/searchy": "^2.0",
        "yajra/laravel-datatables-buttons": "^4.6",
        "yajra/laravel-datatables-html": "^4.4",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/danny50610/Imgur-Laravel"
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "spatie/laravel-html"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
